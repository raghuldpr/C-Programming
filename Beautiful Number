#include<stdio.h>

int main()
{
    int n, sum;

    printf("Enter a number: ");
    scanf("%d", &n);

    while(n > 9)   // Repeat until single digit
    {
        sum = 0;

        while(n != 0)
        {
            sum += n % 10;
            n /= 10;
        }

        n = sum;
    }

    if(n >= 1 && n <= 9)
        printf("Beautiful Number");
    else
        printf("Not a Beautiful Number");

    return 0;
}
